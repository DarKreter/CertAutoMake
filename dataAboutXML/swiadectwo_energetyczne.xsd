<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.pentacomp.pl/xsd/mir-sr/swiadectwa-import"
           xmlns="http://www.pentacomp.pl/xsd/mir-sr/swiadectwa-import">

    <xs:element name="SwiadectwoEnergetyczne" type="swiadectwoEnergetyczne"/>

    <xs:complexType name="swiadectwoEnergetyczne">
        <xs:sequence>
            <xs:element name="danePodstawowe" type="danePodstawowe" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Oceniany budynek
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="wazneDo" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Ważne do (rrrr-mm-dd)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="energiaKoncowa" type="energiaKoncowa" nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Wskaźnik rocznego zapotrzebowania na energię końcową EK [kWh/(m2·rok)]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="energiaPierwotna" type="energiaPierwotna" nillable="true" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Wskaźnik rocznego zapotrzebowania na nieodnawialną energię pierwotną EP [kWh/(m2·rok)]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="energiaUzytkowa" type="energiaUzytkowa" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Wskaźnik rocznego zapotrzebowania na energię użytkową EU [kWh/(m2·rok)]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="systemOswietlenia" type="roczneZuzycieNosnikaEnergiiSystemuOswietlenia" nillable="true"
                        minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        System wbudowanej instalacji oświetlenia
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="systemChlodzenia" type="roczneZuzycieNosnikaEnergiiSystemuChlodzenia" nillable="true"
                        minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        System chłodzenia
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="systemPrzygotowaniaCieplejWody" type="roczneZuzycieNosnikaEnergiiSystemuCieplejWody"
                        nillable="true" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        System przygotowania ciepłej wody użytkowej
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="systemOgrzewczy" type="roczneZuzycieNosnikaEnergiiSystemuOgrzewania" nillable="true"
                        minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        System ogrzewczy
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="uwagiDoSwiadectwa" type="uwagiDoSwiadectwa" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Zalecenia
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="przegrodyBudynku" type="przegrodaBudynku" nillable="true" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Nazwa przegrody budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="podzialPowierzchniUzytkowej" type="podzialPowierzchniUzytkowej" nillable="true"
                        minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Podział powierzchni użytkowej budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="stacjaKlimatyczna" type="stacjaKlimatyczna" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Stacja meteorologiczna, według której danych jest wyznaczana charakterystyka energetyczna
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paramTech" type="parametryTechniczne" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Podstawowe parametry techniczno-użytkowe budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="parametrTechinczySystemOgrzewczy" type="parametrTechinczySystemOgrzewczy" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Element określający system ogrzewczy w podstawowych parametrach techniczno-użytkowych budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="parametrTechinczySystemCieplejWody" type="parametrTechinczySystemCieplejWody" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Element określający system ciepłej wody w podstawowych parametrach techniczno-użytkowych budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="parametrTechinczySystemChlodzenia" type="parametrTechinczySystemChlodzenia" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Element określający system chłodzenia w podstawowych parametrach techniczno-użytkowych budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="charakterystykaEnergetyczna" type="charakterystykaEnergetyczna" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Ocena charakterystyki energetycznej budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="swiadectwoEnergetyczneType" type="swiadectwoEnergetyczneType">
                <xs:annotation>
                    <xs:documentation>
                        Element określający rodzaj świadectwa (zgodnie z opisem enumeracji typu danych)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="swiadectwoEnergetyczneImage" type="swiadectwoEnergetyczneImage" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Zdjęcie budynku w base64, zdjęcie musi spełniać poniższe kryteria:
						-maksymalna proporcja 0.8, minimalna proporcja 0.65 (proporcja jest wyliczana na podstawie wzoru: szerokość/wysokość )
						-maksymalny rozmiar zdjęcia 100 kB
						-obsługiwane typy zdjęć jpeg, gif, png, tif
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="danePodstawowe">
        <xs:sequence>
            <xs:element name="miejscowosc" type="string70" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Miejscowość
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ulica" type="string200" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Ulica
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nrDomu" type="string10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nr domu
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nrLokalu" type="string10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nr lokalu
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="kodPocztowy" type="string10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Kod Pocztowy
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poczta" type="string70" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Poczta
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="wojewodztwo" type="wojewodztwo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Województwo
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="terytPowiat" type="powiat" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Kod teryt powiatu
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="terytGmina" type="gmina" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Kod teryt gminy
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="czyBudynek" type="yesNoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Budynek zajmowany przez władze publiczne
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="czyBudynekZajmowanyPrzezOrganWiekszyOd250" type="yesNoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Budynek, w którym powierzchnia użytkowa zajmowana przez organy wymianu sprawiedliwości,
                        prokuraturę lub organy administracji publicznej przekracza 250 m2
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="metodaObliczaniaCharakterystyki" type="metodaObliczaniaCharakterystykiEnergetycznej"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Metoda wyznaczania charakterystyki energetycznej
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nazwaOrganuZajmujacegoBudynek" type="string300" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nazwa organu zajmującego budynek
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powierzchUzytkowa" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Powierzchnia użytkowa budynku/modułu budynku [m2]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powierzchniaRegulowanaTemp" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Powierzchnia o regulowanej temperaturze [Ar, m2]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powierzchniaZajmowanaPrzezOrgan" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Powierzchnia zajmowana przez organ
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="przeznaczenieBudynku" type="przeznaczenieBudynku" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Przeznaczenie budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="przeznaczenieBudynkuInny" type="string40" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Inne przeznaczenie budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rodzajBudynku" type="rodzajBudynku" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Rodzaj budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rokOddania" type="xs:int" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Rok oddania do użytkowania
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="uwagiDoSwiadectwa">
        <xs:sequence>
            <xs:element name="inneUwagi" type="string4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Inne uwagi dotyczące poprawy charakterystyki energetycznej budynku (w tym wskazanie, gdzie można
                        uzyskać szczegółowe informacje dotyczące opłacalności ekonomicznej zawartych w świadectwie oraz
                        informację dotyczącą działań, jakie należy podjąć w celu wypełnienia zaleceń)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zaleceniaDoPrzegrodBudynku" type="string4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Zalecenia dotyczące opłacalnej ekonomicznie poprawy charakterystyki energetycznej lokalu
                        mieszkalnego w zakresie przegród budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zaleceniaDoPrzegrodBudynkuNiezaleznie" type="string4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Zalecenia dotyczące opłacalnej ekonomicznie poprawy charakterystyki energetycznej części budynku
                        w zakresie przegród budynku niezależnie od planowanych robót budowlanych, o których mowa w pkt 1
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zaleceniaDoSystemowTechnicznych" type="string4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Zalecenia dotyczące opłacalnej ekonomicznie poprawy charakterystyki energetycznej budynku w
                        zakresie systemów technicznych w budynku w przypadku planowania robót budowlanych polegających
                        na ociepleniu budynku, obejmujących ponad 25% powierzchni przegród zewnętrznych tego budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zaleceniaDoSystemowTechnicznychNiezaleznie" type="string4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Zalecenia dotyczące opłacalnej ekonomicznie poprawy charakterystyki energetycznej części budynku
                        w zakresie systemów technicznych w budynku lub części budynku niezależnie od planowanych robót
                        budowlanych, o których mowa w pkt 2
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="energiaPierwotna">
        <xs:complexContent>
            <xs:extension base="energiaValues">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="energiaKoncowa">
        <xs:complexContent>
            <xs:extension base="energiaValues">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="energiaUzytkowa">
        <xs:complexContent>
            <xs:extension base="energiaValues">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="energiaValues">
        <xs:sequence>
            <xs:element name="rodzajNosnika" type="paliwaZrodlaEnergii" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Rodzaj nośnika energii lub energii
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inne" type="string255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Inne
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="oswietlenieWbudowane" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Oświetlenie wbudowane
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="chlodzenie" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Chłodzenie
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cieplaWoda" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Ciepła woda użytkowa
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ogrzewanieWentylacja" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Ogrzewanie i wentylacja
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="przegrodaBudynku">
        <xs:sequence>
            <xs:element name="wspolczynnikPrzenikaniaCieplaWymagany" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Współczynnik przenikania ciepła wymagany U [W/(m2·K)]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="wspolczynnikPrzenikaniaCieplaUzyskany" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Współczynnik przenikania ciepła uzyskany U [W/(m2·K)]
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="opisPrzegrody" type="string4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Opis przegrody
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nazwaPrzegrodyBudynku" type="nazwaPrzegrodyBudynku" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nazwa przegrody budynku
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nazwaPrzegrodyInna" type="string255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nazwa przegrody inna
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="czyBezWymagan" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Umożliwienie uzupełnienia współczynnika przenikania ciepła przegrody słowami bez wymagań
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="roczneZuzycieNosnikaEnergiiSystemuOswietlenia">
        <xs:complexContent>
            <xs:extension base="roczneZuzycieNosnikaEnergii">
